{
  "name": "express-example",
  "version": "1.0.0",
  "description": "Express microservice example",
  "main": "app.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/app.js\"",
    "pretest": "npm run build",
    "test": "restqa run",
    "happy": "restqa run --report",
    "sandbox": "restqa sandbox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaiahmoran/express-example.git"
  },
  "keywords": [
    "example"
  ],
  "author": "Isaiah Moran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isaiahmoran/express-example/issues"
  },
  "homepage": "https://github.com/isaiahmoran/express-example#readme",
  "dependencies": {
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "swagger-jsdoc": "6.2.1",
    "swagger-ui-express": "4.4.0"
  },
  "devDependencies": {
    "@restqa/restqa": "^1.0.0-rc4",
    "@types/express": "4.17.13",
    "@types/node": "18.0.1",
    "concurrently": "7.2.2",
    "nodemon": "2.0.19",
    "typescript": "4.7.4"
  }
